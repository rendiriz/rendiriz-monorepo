{
  "name": "rendiriz-ecosystem",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "*** Development ***": "",
    "dev:personal": "env-cmd -f apps/personal/.env.local nx serve personal",
    "dev:generate:personal": "env-cmd -f apps/personal/.env.local npm run prisma:generate:personal",
    "dev:migrate:personal": "env-cmd -f apps/personal/.env.local npm run prisma:migrate:personal",
    "*** Production ***": "",
    "build:prod:personal": "nx run personal:build:production",
    "*** Prisma ***": "",
    "prisma:generate:personal": "npx prisma generate --schema=apps/personal/prisma/schema.prisma",
    "prisma:migrate:personal": "npx prisma migrate dev --schema=apps/personal/prisma/schema.prisma",
    "*** Partytown ***": "",
    "partytown:personal": "partytown copylib apps/personal/public/~partytown",
    "*** Other ***": "",
    "prepare": "husky install",
    "sanity-start": "sanity start"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
