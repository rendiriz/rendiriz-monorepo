    "apps/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "libs/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "apps/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "libs/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "package.json": [
      "lockfile-lint --path package-lock.json --type npm --allowed-hosts npm yarn --validate-https"
    ]
